<configuration>
    <appender name="console" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{yyyy-MM-dd HH:mm:ss} - %msg%n</pattern>
        </encoder>
    </appender>
    <root level="INFO">
        <appender-ref ref="console" />
    </root>
    <logger name="zw.org.mohcc.hiv_dashboard" level="DEBUG" additivity="false">
        <appender-ref ref="console" />
    </logger>
</configuration>
